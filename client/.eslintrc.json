{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "plugin": ["prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": "off",
    "comma-dangle": "off",
    "import/no-unresolved": "off",
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": "warn",
    "react/jsx-no-bind": "off",
    "react/prop-types": "Off"
  }
}
